{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "straightvisions/sv-slider-slide",
	"parent": ["straightvisions/sv-slider"],
	"title": "SV Slider Slide",
	"icon": "slides",
	"category": "media",
	"description": "Create a slick and responsive Slider.",
	"keywords": [ "video", "image", "gallery", "slider" ],
	"textdomain": "sv-slider",
	"editorScript": "sv-slider-editor-script",
	"editorStyle": "sv-slider-editor-style",
	"style": "sv-slider-style",
	"attributes": {
		"uid": {
			"type": "string"
		},
		"controls": {
			"type": "boolean"
		},
		"arrows": {
			"type": "boolean"
		},
		"bullets": {
			"type": "boolean"
		},
		"autoplay": {
			"type": "boolean"
		},
		"loop": {
			"type": "boolean"
		}
	},
	"supports": {
		"anchor": true
	}
}