{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "straightvisions/sv-slider",
	"title": "SV Slider",
	"icon": "slides",
	"category": "media",
	"description": "Create a slick and responsive Slider.",
	"keywords": [
		"video",
		"image",
		"gallery",
		"slider"
	],
	"textdomain": "sv-slider",
	"editorScript": "sv-slider-editor-script",
	"editorStyle": "sv-slider-editor-style",
	"style": "sv-slider-style",
	"attributes": {
		"uid": {
			"type": "string"
		},
		"controls": {
			"type": "boolean"
		},
		"arrows": {
			"type": "boolean"
		},
		"bullets": {
			"type": "boolean"
		},
		"autoplay": {
			"type": "boolean"
		},
		"loop": {
			"type": "boolean"
		},
		"editMode": {
			"type": "boolean",
			"default": true
		},
		"innerBlocks": {
			"type": "array",
			"default": []
		},
		"innerContent": {
			"type": "string",
			"default": ""
		},
		"tagName": {
			"type": "string",
			"default": "div"
		},
		"svSlider": {
			"type": "array",
			"default": {}
		}
	},
	"supports": {
		"anchor": true,
		"align": [
			"full",
			"wide"
		]
	}
}